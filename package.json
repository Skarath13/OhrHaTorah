{
  "name": "ohrhatorah",
  "type": "module",
  "version": "0.0.1",
  "scripts": {
    "dev": "astro dev",
    "build": "astro build",
    "preview": "astro preview",
    "astro": "astro",
    "db:create": "wrangler d1 create ohrhatorah-db",
    "db:migrate": "wrangler d1 execute ohrhatorah-db --file=./schema.sql",
