    "r2:create": "wrangler r2 bucket create ohrhatorah-images"
  },
  "dependencies": {
    "@astrojs/cloudflare": "^12.6.11",
    "astro": "^5.16.0",
    "bcryptjs": "^2.4.3"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "wrangler": "4.40.0"
  }
}
